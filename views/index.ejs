                                        <option>Make an Order</option>
                                        <option>Plan your Event</option>
                                        <option>Homemade recipe supply</option>
                                        <option>Food Delivery</option>
                                    </select>
                                </div>
                                <div class="form-group py-2">
                                    <label for="InputMessage">Write us a Message</label>
                                    <textarea class="form-control" id="InputMessage" rows="3" name="message"></textarea>
                                </div>
                                <button type="submit" class="btn btn-warning" style="margin-left: auto; margin-right: auto;">Submit</button>
